    "import torch\n",
    "\n",
    "# 获取 Torch Hub 缓存根目录\n",
    "hub_cache_dir = torch.hub.get_dir()\n",
    "print(f\"Torch Hub 缓存位置: {hub_cache_dir}\")\n",
    "\n",
    "# 也可以通过环境变量直接查看（终端/代码中都可）\n",
    "import os\n",
    "hub_env = os.environ.get('TORCH_HOME', os.path.expanduser('~/.cache/torch'))\n",
    "print(f\"TORCH_HOME 环境变量指定的缓存位置: {hub_env}\")"
   ]
